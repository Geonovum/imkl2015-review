<?xml version="1.0" encoding="ISO-8859-1"?>
<StyledLayerDescriptor version="1.0.0"
    xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.0.0/StyledLayerDescriptor.xsd"
    xmlns="http://www.opengis.net/sld"
    xmlns:ogc="http://www.opengis.net/ogc"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <NamedLayer>
        <Name>informatiepolygoon</Name>
        <UserStyle>
            <Title>Informatiepolygoon</Title>
            <Abstract>Deze stylesheet bevat de regels voor de visualisatie van de informatiepolygoon</Abstract>
            <FeatureTypeStyle>
                <Rule>
                    <Name>informatiepolygoon</Name>
                    <Title>Informatiepolygoon</Title>
                    <PolygonSymbolizer>
                        <Fill>
                            <CssParameter name="fill">#ff00ff</CssParameter>
                            <CssParameter name="fill-opacity">0.35</CssParameter>
                        </Fill>
                    </PolygonSymbolizer>
                </Rule>
            </FeatureTypeStyle>
        </UserStyle>
    </NamedLayer>
</StyledLayerDescriptor>
