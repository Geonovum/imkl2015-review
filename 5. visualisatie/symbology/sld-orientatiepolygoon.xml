    <NamedLayer>
        <Name>orientatiepolygoon</Name>
        <UserStyle>
            <Title>Oriëntatiepolygoon</Title>
            <Abstract>Deze stylesheet bevat de regels voor de visualisatie van de oriëntatiepolygoon</Abstract>
            <FeatureTypeStyle>
                <Rule>
                    <Name>graafpolygoon</Name>
                    <Title>Graafpolygoon</Title>
                    <LineSymbolizer>
                        <Stroke>
                            <CssParameter name="stroke">#ff00ff</CssParameter>
