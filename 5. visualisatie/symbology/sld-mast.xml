                    </ogc:Filter>
                    <MinScaleDenominator>0</MinScaleDenominator>
                    <MaxScaleDenominator>5000</MaxScaleDenominator>
                    <PointSymbolizer>
                        <Graphic>
							<ExternalGraphic>
								<OnlineResource xlink:type="simple" xlink:href="mast_LS.svg"/>
								<Format>image/svg+xml</Format>
							</ExternalGraphic>
                        </Graphic>
                    </PointSymbolizer>
                </Rule>
